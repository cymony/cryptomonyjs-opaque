  "compilerOptions": {
    "target": "esnext",
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "typeRoots": ["node_modules/@types/"]
  }
}
